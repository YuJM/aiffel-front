{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ui": [
        "src/ui"
      ],
      "@ui/*": [
        "src/ui/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@util/*": [
        "src/util/*"
      ],
      "@model": [
        "src/model"
      ],
      "@model/*": [
        "src/model/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@store": [
        "src/store"
      ],
      "@store/*": [
        "src/store/*"
      ]
    }
  }
}
